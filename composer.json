{
  "name": "nikapps/nikpay-laravel",
  "description": "A laravel 5 package for connecting to Iranian banks for payment transactions",
  "keywords": [
    "laravel",
    "nikpay",
    "shaparak",
    "saman",
    "pardakht",
    "dargah",
    "payment",
    "bank",
    "iran"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ali Borhani",
      "email": "aliborhani1@gmail.com"
    },
    {
      "name": "Hossein Moradgholi",
      "email": "hossein@moradgholi.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=5.6",
    "ext-soap": "*",
    "illuminate/support": "^5.1,<5.5",
    "illuminate/config": "^5.1,<5.5",
    "illuminate/contracts": "^5.1,<5.5",
    "nikapps/nikpay": "^1.0.5"
  },
  "require-dev": {
    "orchestra/testbench": "~3.4",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Nikapps\\NikPayLaravel\\": "src/",
      "Nikapps\\NikPayLaravel\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
